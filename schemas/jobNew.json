{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/companyUpdate.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 0
    },
    "salary": {
      "type": ["number", "null"]
    },
    "equity": {
      "type": ["number", "null"],
      "minimum": 0,
      "maximum": 1
    },
    "companyHandle": {
      "type": "string",
      "minLength": 1
    }
  },
  "additionalProperties": false,
  "required": ["title", "salary", "equity", "companyHandle"]
}
