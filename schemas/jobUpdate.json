{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/companyUpdate.schema.json",
  "type": "object",
  "properties": {
    "id": {
      "type": "number",
      "minimum": 1
    },
    "title": {
      "type": "string",
      "minLength": 0
    },
    "salary": {
      "type": "integer"
    },
    "equity": {
      "type": "number",
      "minimum": 0,
      "maxItems": 1
    },
    "companyHandle": {
      "type": "string",
      "minLength": 1
    }
  },
  "additionalProperties": false,
  "required": ["id"]
}
